          <div class="goToExhibitorDetails slds-m-top_medium">
            <button
              class="slds-button slds-button_destructive full-width-button"
              onclick="{!c.navigateToExhibitor}"
              >{!$Label.c.View + ' ' + $Label.c.Exhibitor_Details}</button
            >
          </div>
        </div>
      </div>
    </section>
    <aura:if isTrue="{!!v.modalInModal}">
      <div class="slds-backdrop slds-backdrop_open"></div>
    </aura:if>
  </aura:if>
  <!-- Modal for Exhibitor QuickView End -->

  <!-- Modal for Manage disable Start -->
  <div id="modalManageDisable" class="modal">
    <div class="modal-content">
      <span onclick="{!c.hideExhibitorDisableTooltip}" class="close"
        >&times;</span
      >

      <div
        class="modal-body bodycompany "
        style="max-height: 68vh;padding-right:5px;overflow-y: auto;overflow-x: hidden"
      >
        <div class="slds-grid slds-wrap">
          <div class="slds-col slds-size_1-of-9 slds-m-right_small">
            <div class="tooltip">
              <i class="fa fa-question-circle" style="font-size: large;"></i>
            </div>
          </div>
          <div class="slds-col slds-size_7-of-8">
            <b>{!$Label.c.Manage_Exhibitor_Reason}</b>
            <br />
            <br />

            {!$Label.c.Manage_Exhibitor_Reason_Explanation}
            <br />
            <br />
          </div>
        </div>
      </div>
    </div>
  </div>
  <!-- Modal for Manage disable End -->
</aura:component>